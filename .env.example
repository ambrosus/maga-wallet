# Web3Auth Configuration
# Client ID from Web3Auth
W3A_CLIENT_ID=your_web3auth_client_id_here

# Provider configurations for Web3Auth
W3A_GOOGLE_PROVIDER=your_google_provider_here
W3A_APPLE_PROVIDER=your_apple_provider_here
W3A_TWITTER_PROVIDER=your_twitter_provider_here

# Firebase Configuration
# OAuth Client ID for Firebase authentication
FIREBASE_OAUTH_CLIENT_ID=your_firebase_oauth_client_id_here
FIREBASE_WEB_CLIENT_ID=your_firebase_web_client_id_here

# X (Twitter) API Credentials
X_CONSUMER_KEY=your_x_consumer_key_here
X_CONSUMER_SECRET=your_x_consumer_secret_here